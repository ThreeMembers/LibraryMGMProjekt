<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.borrowTask.borrowRequestItemController">
   <children>
      <HBox prefHeight="40.0" prefWidth="880.0">
         <children>
            <Label fx:id="lbID" alignment="CENTER" prefHeight="40.0" prefWidth="80.0" text="ID" />
            <Label fx:id="lbReader" alignment="CENTER" prefHeight="40.0" prefWidth="400.0" text="Reader" />
            <Label fx:id="lbDate" alignment="CENTER" prefHeight="40.0" prefWidth="400.0" text="Date" />
         </children>
      </HBox>
      <Button fx:id="detailButton" blendMode="MULTIPLY" mnemonicParsing="false" prefHeight="40.0" prefWidth="880.0" style="-fx-background-color: white;" />
   </children>
</StackPane>
