<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.borrowTask.AddBorrowRequestItemController">
   <children>
      <HBox>
         <children>
            <Label fx:id="lbBook" alignment="CENTER" prefHeight="40.0" prefWidth="400.0" text="Book name" />
            <Label fx:id="lbQuantity" alignment="CENTER" prefHeight="40.0" prefWidth="400.0" text="Quantity" />
            <Label alignment="CENTER" prefHeight="40.0" prefWidth="78.0">
               <graphic>
                  <CheckBox fx:id="checkbox" mnemonicParsing="false" onAction="#checked" scaleX="1.5" scaleY="1.5" />
               </graphic>
            </Label>
         </children>
      </HBox>
   </children>
</StackPane>
