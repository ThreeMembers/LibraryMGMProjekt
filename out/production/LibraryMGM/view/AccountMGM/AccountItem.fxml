<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AccountMGM.AccountItem">
    <children>
        <HBox prefHeight="40.0" prefWidth="880.0">
            <children>
                <Label fx:id="lbID" alignment="CENTER" prefHeight="40.0" prefWidth="80.0" text="ID" />
                <Label fx:id="lbPermission" alignment="CENTER" prefHeight="40.0" prefWidth="100.0" text="Permission" />
                <Label fx:id="lbUserName" alignment="CENTER" prefHeight="40.0" prefWidth="125.0" text="UserName" />
                <Label fx:id="lbRealName" alignment="CENTER" prefHeight="50.0" prefWidth="125.0" text="RealName" />
                <Label fx:id="lbAge" alignment="CENTER" prefHeight="50.0" prefWidth="100.0" text="Age" />
                <Label fx:id="lbGender" alignment="CENTER" prefHeight="50.0" prefWidth="100.0" text="Gender" />
                <Label fx:id="lbExpirationDate" alignment="CENTER" prefHeight="50.0" prefWidth="125.0" text="ExpirationDate" />
                <Label fx:id="lbDateLeft" alignment="CENTER" prefHeight="50.0" prefWidth="125.0" text="DateLeft" />
            </children>
        </HBox>
        <Button fx:id="detailButton" blendMode="MULTIPLY" mnemonicParsing="false" prefHeight="40.0" prefWidth="880.0" style="-fx-background-color: white;" />
    </children>
</StackPane>
