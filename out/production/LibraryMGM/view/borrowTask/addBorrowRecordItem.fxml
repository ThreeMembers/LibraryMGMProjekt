<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.borrowTask.AddBorrowRecordItemController">
   <children>
      <HBox>
         <children>
            <Label fx:id="lbName" prefHeight="40.0" prefWidth="650.0" text="Book name">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="10.0" />
               </padding></Label>
            <Label fx:id="lbStock" alignment="CENTER" prefHeight="40.0" prefWidth="150.0" text="ID Stock" />
            <Label alignment="CENTER" prefHeight="40.0" prefWidth="78.0">
               <graphic>
                  <CheckBox fx:id="checkbox" mnemonicParsing="false" onAction="#checked" scaleX="1.5" scaleY="1.5" />
               </graphic>
            </Label>
         </children>
      </HBox>
   </children>
</StackPane>
