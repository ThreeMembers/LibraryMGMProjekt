<StackPane fx:id="container" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InputandRemove.DetailDeItemController">
    <children>
        <HBox prefHeight="40.0" prefWidth="880.0">
            <children>
                <Label fx:id="lbRecord" alignment="CENTER" prefHeight="40.0" prefWidth="440.0" text="Record" />
                <Label fx:id="lbStock" alignment="CENTER" prefHeight="40.0" prefWidth="440.0" text="Stock" />
            </children>
        </HBox>
        <Button fx:id="detailButton" blendMode="MULTIPLY" mnemonicParsing="false" prefHeight="40.0" prefWidth="880.0" style="-fx-background-color: #f5cd79;" />
    </children>
</StackPane>
