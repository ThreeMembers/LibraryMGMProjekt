<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StockBookViewItemController">
   <children>
      <HBox>
         <children>
            <Label fx:id="lbID" alignment="CENTER" prefHeight="40.0" prefWidth="80.0" text="Label">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="lbBook" prefHeight="40.0" prefWidth="400.0" text="Book Name">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Label fx:id="lbQuality" alignment="CENTER" prefHeight="40.0" prefWidth="176.0" text="Quality">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Label fx:id="lbRelease" alignment="CENTER" prefHeight="40.0" prefWidth="110.0" text="Release Year">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Label fx:id="lbIsBorrow" alignment="CENTER" prefHeight="40.0" prefWidth="110.0" text="Is Boorow">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font></Label>
         </children>
      </HBox>
      <Button fx:id="detailButton" blendMode="MULTIPLY" mnemonicParsing="false" prefHeight="40.0" prefWidth="880.0" style="-fx-background-color: white;" />
   </children>
</StackPane>
