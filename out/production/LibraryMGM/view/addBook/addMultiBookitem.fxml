<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="container" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.addBook.AddMultiBookitemController">
   <children>
      <HBox>
         <children>
            <Label fx:id="lbName" alignment="CENTER" prefHeight="40.0" prefWidth="400.0" text="Book Name " />
            <Label fx:id="lbAuthor" alignment="CENTER" prefHeight="40.0" prefWidth="200.0" text="Author" />
            <Label fx:id="lbCate" alignment="CENTER" prefHeight="40.0" prefWidth="200.0" text="Category" />
            <StackPane prefHeight="40.0" prefWidth="78.0">
               <children>
                  <CheckBox fx:id="checkBox" mnemonicParsing="false" onAction="#checked" />
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
</StackPane>
